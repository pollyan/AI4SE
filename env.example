# AI模型配置 - 请复制此文件为.env并填入真实的API密钥

# OpenAI配置 (推荐用于UI自动化)
# OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_BASE_URL=https://api.openai.com/v1
# MIDSCENE_MODEL_NAME=gpt-4o

# 或者使用其他模型，如Qwen (推荐用于UI自动化)
OPENAI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
OPENAI_API_KEY=your_qwen_api_key_here
MIDSCENE_MODEL_NAME=qwen-vl-max-latest
MIDSCENE_USE_QWEN_VL=1

# 或者使用Gemini
# OPENAI_BASE_URL=https://generativelanguage.googleapis.com/v1beta
# OPENAI_API_KEY=your_gemini_api_key_here
# MIDSCENE_MODEL_NAME=gemini-2.0-flash-exp

# 浏览器配置
HEADLESS=false
TIMEOUT=30000

# 数据库配置 - 仅支持PostgreSQL
# 本系统仅支持PostgreSQL数据库，必须配置以下环境变量之一：

# 方式1：使用通用的DATABASE_URL
DATABASE_URL=postgresql://postgres:[password]@[host]:5432/[database]

# 方式2：使用Supabase专用配置
# 从Supabase项目设置 -> Database -> Connection string 获取
# SUPABASE_DATABASE_URL=postgresql://postgres:[password]@[host]:5432/[database]

# 本地开发环境示例：
# DATABASE_URL=postgresql://postgres:password@localhost:5432/intent_test

# Supabase项目配置 (可选，用于Supabase客户端SDK)
# SUPABASE_URL=https://[project-id].supabase.co
# SUPABASE_ANON_KEY=your_anon_key_here

# 应用配置
SECRET_KEY=your_secret_key_here
DEBUG=false

# 文件路径配置
SCREENSHOT_DIR=web_gui/static/screenshots
LOG_DIR=logs